n\r\n  useEffect(() => {\r\n    LoginSchema.isValid(loginData).then((valid) => {\r\n      setDisabled(!valid);\r\n    });\r\n  }, [loginData]);\r\n\r\n  const login = (user) => {\r\n    axiosWithAuth()\r\n      .post('login', user)\r\n      .then((res) => {\r\n        console.log(res);\r\n        sessionStorage.setItem('token', res.data.token);\r\n        push('/issues');\r\n      })\r\n      .catch((err) => {\r\n        console.log(err, 'ERROR');\r\n      });\r\n  };\r\n\r\n  // / / / / / Submit Handler for Login Functionality / / / / / //\r\n\r\n  const handleSubmit = (event) => {\r\n    event.preventDefault();\r\n    const user = { ...loginData }; //this line is redundant, see me after class -Peter\r\n    console.log('LOGIN DATA', loginData);\r\n    login(user);\r\n    // login(loginData);\r\n\r\n    setLoginData(initLoginValues);\r\n  };\r\n\r\n  // / / / / / Above currently returning 401 error - leaving for later use / / / / / //\r\n\r\n  return (\r\n    <>\r\n      <NavBar />\r\n\r\n      <LoginDiv>\r\n        <div className=\"text\">\r\n          <h2>Log in here!</h2>\r\n        </div>\r\n                \r\n        <LoginForm onSubmit={handleSubmit}>\r\n          <label>\r\n            User Name / Email: <br />\r\n            <Error>{errors.username}</Error> <br />\r\n                            \r\n            <input\r\n              // type=\"text\"\r\n              name=\"username\"\r\n              value={loginData.username}\r\n              onChange={handleChange}\r\n            />\r\n                        \r\n          </label>\r\n                      \r\n          <label>\r\n            Password: <br />\r\n            <Error>{errors.password}</Error> <br />\r\n                            \r\n            <input\r\n              type=\"password\"\r\n              name=\"password\"\r\n              value={loginData.password}\r\n              onChange={handleChange}\r\n            />\r\n                        \r\n          </label>\r\n          <Style